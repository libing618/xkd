    <view class="weui-cell__hd{{p}}:</view>
    <view class="weui-cell__ft"><icon id="se-{{n}}" bindtap="i_cargopackage" type="{{inclose ? 'info' : 'success_no_circle'}}" size="20"/></view>
  </view>
  <view hidden="{{inclose}}" class="weui-cells">
