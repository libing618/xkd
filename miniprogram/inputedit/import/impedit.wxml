    <button class="weui-search-bar__cancel-btn" type="primary" size="mini" id="sc-{{n}}" bindtap="i_inScan">扫码</button>
  </view>
</template>

<template name="i_insdata">
  <view class="weui-footer_fixed-bottom weui-grids">
