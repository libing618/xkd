.navigationBar {
  position: fixed;
  top:0;
  z-index: 500;
  width: 100%;
  background-color: #F5F5F5;
  border-bottom: 1rpx solid #CCCCCC;
}
.navigationBar__column {
  position: relative;
  margin-left: 20rpx;
  white-space: nowrap;
  font-size: 32rpx;
  width: 540rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.navigationBar__icon {
  margin-right: 10rpx;
  vertical-align: middle;
  width: 60rpx;
  height: 60rpx;
}
.navigationBar__item {
  position: relative;
  width: 10em;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  padding: 6rpx 0;
  text-align: center;
}
.navigationBar__title {
  display: inline-block;
  font-size: 30rpx;
  max-width: 10em;
  width: auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-wrap: normal;
}
.navigationBar__btn {
  position: inline-block;
  padding-left: 8rpx;
  padding-right: 8rpx;
  line-height: 46rpx;
  font-size: 26rpx;
  background-color:#FFFFFF;
  border-radius: 60rpx;
  border-color: #FFFFFF;
  text-align: center;
}
.navSearchBar__form {
  position: relative;
  word-wrap: break-word;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 480rpx;
  border-radius: 30rpx;
  background-color: #FFFFFF;
  border: 1rpx solid #E6E6EA;
}
.navSearchBar__input {
  height:1.4rem;
  text-overflow:clip;
  overflow:hidden;
  white-space:nowrap;
  width:400rpx;
}
.navigationBar__btn_icon {
  position: inline-block;
  margin-top: 0rpx;
	margin-right: 10rpx;
  padding: 0 0;
  line-height: 46rpx;
  width: 50rpx;
  background-color:#FFFFFF;
  border-color: #FFFFFF;
  text-align: center;
}
.navigationBar__price {
  display: inline-block;
	color: #f60;
  margin-top: 10rpx;
	margin-left: 10rpx;
  font-size: 34rpx;
}

.page__hd {
  padding-top: 10rpx;
}
.page__bd {
  padding-bottom: 10rpx;
}
.page__bd_spacing {
  padding-left: 16rpx;
  padding-right: 16rpx;
}
.page__ac_spacing {
  padding-left: 40rpx;
  padding-right: 40rpx;
}
.page__ft{
  padding-bottom: 10rpx;
  text-align: center;
}

.page__title {
  text-align: center;
  font-size: 36rpx;
  font-weight: 400;
}

.page__desc {
  margin-top: 6rpx;
  color: #888888;
  text-align: left;
  font-size: 28rpx;
}
.chat-box__left {
  padding-top: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  float: left;
}
.chat-box__right {
  padding-top: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  float: right;
}
.chat-box__hd {
  margin-right: 20rpx;
  margin-left: 20rpx;
  width: 120rpx;
  line-height: 120rpx;
  text-align: center;
}
.chat-box__bd {
  width: 600rpx;
  line-height: 120rpx;
  text-align: left;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}

.modal_box {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  color: #fff;
  background-color: #FFFFFF;
  -moz-opacity: 0.8;
  opacity: 0.8;
  z-index: 600;
  filter: alpha(opacity=40);
}

.modal_attr_box {
  width: 100%;
  overflow: scroll;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 700;
  border: 6rpx solid lightgray;
  border-top-left-radius: 8rpx;
  border-top-right-radius: 8rpx;
  background: silver;
}

.close_icon {
  width: 25rpx;
  height: 25rpx;
  line-height: 20rpx;
  font-size: 30rpx;
  position: absolute;
  right: 5rpx;
  top:5rpx;
  padding: 10rpx;
  color: red;
  border: 1rpx solid #ccc;
  border-radius: 50%;
  text-align: center;
  background: #fff;
}

.drawer_attr_content {
  color: #232626;
	font-size: 32rpx;
	background-color: #f9f9f9;
}

.list {
  margin-top: 40rpx;
  height: auto;
  width: 100%;
  background: #fff;
  padding: 0 40rpx;
  border: 1px solid rgba(0, 0, 0, 0.1);
  border-left: none;
  border-right: none;
  transition: all 300ms ease;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  box-sizing: border-box;
}

.list-item {
  width: 100%;
  padding: 0;
  line-height: 104rpx;
  font-size: 34rpx;
  color: #007aff;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
  display: flex;
  flex-direction: row;
  align-content: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.list-item:first-child {
  border-top: none;
}

.list-item image {
  max-width: 100%;
  max-height: 20vh;
  margin: 20rpx 0;
}

.request-text {
  color: #222;
  padding: 20rpx 0;
  font-size: 24rpx;
  line-height: 36rpx;
  word-break: break-all;
}

.guide {
  width: 100%;
  padding: 40rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.guide .headline {
  font-size: 34rpx;
  font-weight: bold;
  color: #555;
  line-height: 40rpx;
}

.guide .p {
  margin-top: 20rpx;
  font-size: 28rpx;
  line-height: 36rpx;
  color: #666;
}

.guide .code {
  margin-top: 20rpx;
  font-size: 28rpx;
  line-height: 36rpx;
  color: #666;
  background: white;
  white-space: pre;
}

.guide .code-dark {
  margin-top: 20rpx;
  background: rgba(0, 0, 0, 0.8);
  padding: 20rpx;
  font-size: 28rpx;
  line-height: 36rpx;
  border-radius: 6rpx;
  color: #fff;
  white-space: pre
}

.guide image {
  max-width: 100%;
}

.guide .image1 {
  margin-top: 20rpx;
  max-width: 100%;
  width: 356px;
  height: 47px;
}

.guide .image2 {
  margin-top: 20rpx;
  width: 264px;
  height: 100px;
}

.guide .flat-image {
  height: 100px;
}

.guide .code-image {
  max-width: 100%;
}

.guide .copyBtn {
  width: 180rpx;
  font-size: 20rpx;
  margin-top: 16rpx;
  margin-left: 0;
}

.guide .nav {
  margin-top: 50rpx;
  display: flex;
  flex-direction: row;
  align-content: space-between;
}

.guide .nav .prev {
  margin-left: unset;
}

.guide .nav .next {
  margin-right: unset;
}
