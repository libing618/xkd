<template name="navBarLogo">
  <view class="weui-cell__hd">
    <image src="../../images/logo.png" class="navigationBar__icon"></image>
  </view>
</template>

<template name="navBackIcon">
  <view class="weui-cell__hd">
    <navigator open-type="{{rtUrl ? 'switchTab' : 'navigateBack'}}" url="{{rtUrl}}" delta="1">
      <image src="data:image/png;base64," class="navigationBar__icon"></image>
    </navigator>
  </view>
</template>
