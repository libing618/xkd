value=<import src="wedetail.wxml" />
<wxs module="mDot">
module.exports =
  function(i,cArray){ return i==cArray.length-1 ? '' : '、' }
</wxs>
<wxs module="mid">
module.exports = ['T', 'p','sg','so', 'Geo', '-1', '-2', '-3', '-4','-5']
</wxs>
