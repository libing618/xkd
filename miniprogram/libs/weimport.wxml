</template>

<template name="s_h4">
  <view class="weui-cell" style="font-size: 24rpx;">
    <view class="weui-cell__hd">{{p}}:</view>
    <view class="weui-cell__bd">{{c}}</view>
  </view>
</template>

<template name="s_iNd">
<view class="weui-cell">
  <view class="weui-cell__hd">{{p}}:</view>
  <view class="weui-cell__bd weui-flex">{{mDot(c.uName)}}</view>
</view>
</template>

