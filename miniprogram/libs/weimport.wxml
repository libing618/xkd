</template>

<template name="s_-6">
  <view class="weui-label">{{p}}</view>
  <thumbnail-view-edit name="{{gname}}" csc="{{csc}}" value="{{c}}" editable="0"></thumbnail-view-edit>
</template>

<template name="s_are">
  <array-view-edit name="{{gname}}" c="{{c}}" p="{{p}}" editable="0"></array-view-edit>
</template>

<template name="s_ast">
  <view class="weui-cell">
    <view class="weui-cell__hd">{{p}}:</view>
    <view class="weui-cell__bd">{{c.uName}}</view>
  </view>
</template>

<template name="s_cId">
  <field-view pno="{{gname}}" c="{{c}}" p="{{p}}"></field-view>
</template>

<template name="s_digit">
  <view class="weui-cell">
    <view class="weui-cell__hd">{{p}}:</view>
    <view class="weui-cell__bd">{{c}}</view>
  </view>
</template>

<template name="s_dVE">
  <date-view-edit name="{{gname}}" value="{{c}}" p="{{p}}"></date-view-edit>
</template>

<template name="s_eDetail">
  <view class="weui-article">
    <block wx:for="{{c}}" wx:key="{{index}}">
      <template is="d{{item.t}}" data="{{...item}}"></template>
    </block>
  </view>
</template>

<template name="s_fg">
  <view class="weui-cell">
    <view class="weui-cell__hd">{{p}}:</view>
    <view class="weui-cell__bd">{{c}}</view>
  </view>
</template>

<template name="s_Geo">
  <view class="weui-label">{{p}}</view>
  <address-view-edit name="{{gname}}" value="{{c}}" editable="0"></address-view-edit>
</template>

<template name="s_gSt">
  <view class="weui-cell">{{p}}:</view>
  <view class="weui-cell" id="{{c}}">
    <view class="weui-media-box weui-media-box_appmsg" hover="true" hover-class="weui-cell_active">
      <view class="weui-media-box__hd_in-appmsg">
        <image class="weui-media-box__thumb" src="{{gslist.typevale[c].thumbnail}}" background-size="cover"></image>
      </view>
      <view class="weui-media-box__bd_in-appmsg">
        <view class="weui-media-box__title">{{gslist.typevale[c].uName}}</view>
        <view class="weui-media-box__desc">{{gslist.typevale[c].title}}</view>
      </view>
    </view>
  </view>
</template>

<template name="s_h1">
  <view class="weui-article__h1">{{c}}</view>
</template>

<template name="s_h2">
