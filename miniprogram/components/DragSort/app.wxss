.graphic_bar_hint .span{ font-size: 12px; display: inline-block; margin-right: 13px; vertical-align: top;}
.graphic_bar_hint .hint_bar_orange{ color: #ffb230;}
.graphic_bar_hint .hint_bar{color:#ff655b;}
.graphic_bar_hint .hint_line_yellow{color:#ffc700;}
