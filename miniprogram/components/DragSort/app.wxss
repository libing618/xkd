/*tab*/
.tab{ width: 100%; font-size: 14px; overflow: hidden; z-index: 10; position: relative; z-index: 9999;}
.tab .li{ float: left; width: 25%; text-align: center; }
.tab .li span{ display: block;  line-height: 38px; height: 38px; border-bottom: 2px transparent solid; font-size: 15px; color: #646464;}
.tab .li .span,
.tab .li .select_minute{ display: block;  line-height: 38px; height: 38px; border-bottom: 2px transparent solid; font-size: 15px; color: #646464;}
.tab .li.on span,
.tab .li.on .span,
.tab .li.on .select_minute{ border-color: #df3a32; color: #df3a32; font-weight: bold;}
.tab_no_border{border-bottom: 0;}

.tab_blue .li.on span,
.tab_blue .li.on .span,
.tab_blue .li.on .select_minute{ border-color: #3c9ffc; color: #3c9ffc; }

/*分钟选择*/
.ico_arrow_down{ width: 0; height: 0; display: inline-block; border: 5px transparent solid; border-top: 5px #626262 solid; margin-left: 3px; position: relative; top: 2px;}
.tab .li.on .select_minute .ico_arrow_down{ border-top: 5px #3c9ffc solid; }

.tab_2items .li{ width: 50%;}
.tab_3items .li{ width: 33.33%;}
.tab_4items .li{ width: 25%;}
.tab_5items .li{ width: 20%;}
.tab_6items .li{ width: 16.6%;}

.tab_content{background-color: #fff;}
.tab_content_bottom_holder .tab_content{ min-height: 300px; }


/*inner tab*/
.inner_tab{text-align: center; font-size: 14px; margin: 0 0 26px;}
.inner_tab .li{display: inline-block; border: 1px #df3a32 solid; height: 30px; line-height: 30px; padding: 0 14px; color: #df3a32;}
.inner_tab .li:first-child{ border-right: 0; border-radius: 5px 0 0 5px;}
.inner_tab .li:last-child{ border-left: 0; border-radius: 0 5px 5px 0;}
.inner_tab .li.on{background-color: #df3a32; color: #fff;}
.inner_tab_with_margin{margin: 20px 0 15px;}

.inner_tab_fall .li{ border-color: #17A758; color: #17A758;}
.inner_tab_fall .li.on{background-color: #17A758; color: #fff;}


.detail_tab{text-align: center; font-size: 14px; margin: 0 0 10px; width: 100%; overflow: hidden; background-color: #f8f8f8;}
.detail_tab .li{ float: left; width: 50%; border-bottom: 1px #e6e6e6 solid; height: 26px; line-height: 26px; color: #646464; font-size: 12px;}
.detail_tab .li.on{ color: #3c9ffc; border-bottom: 1px #3c9ffc solid;}





/*同类公司数据对比,右三列可滑动*/
.stocks_table{ overflow: hidden; position: relative; z-index: 10;}
.stocks_table .table_col1{  position: relative; z-index: 3;}
.stocks_table .table_col1 .table_item{ padding-left: 16px; line-height: 1;}
.stocks_table .table_col1 .table_title{ text-indent: 16px;}
.stocks_table .table_col2{ position: absolute; left: 40%; top: 0; right: 0; z-index: 5; overflow: hidden;}
.stocks_table .table_col2 .scroller_inside{width: 600px;}
.stocks_table .table_title{ background-color: #f0f0f0; line-height: 36px; height: 36px; font-size: 14px;}
.stocks_table .table_title .span{ float: left; text-align: right; width: 80px; box-sizing: border-box; -webkit-box-sizing: border-box; padding-right: 16px; white-space: nowrap;}
.stocks_table .table_item{ height: 50px; line-height: 50px; font-size: 15px; position: relative; overflow: hidden;}
.stocks_table .table_item3{ font-size: 16px;}/*三列采用16号字体*/
.stocks_table .table_item:last-child:before{ display: none;}
.stocks_table .table_item .span{ float: left; width: 79px; text-align: right; box-sizing: border-box; -webkit-box-sizing: border-box; padding-right: 16px; white-space: nowrap;}
.stocks_table .table_item .span b.num_stop{font-family: Helvetica, sans-serif;}
.stocks_table .table_item a{ position: absolute; left: 0; top: 0; right: 0; bottom: 0; z-index: 10;}
.stocks_table .table_item b{display: block;}

.st_name{display: block; padding-top: 9px; line-height: 1.1; margin-bottom: -1px;}
.st_name_only{ line-height: 50px; padding: 0; margin: 0; }
.st_code{ display: inline-block; font-size: 12px; line-height: 1.1; color: rgba(89, 89, 89, .6);}
.st_tag{ display: inline-block; border: 1px #fd7736 solid; color: #fd7736; font-size: 8px; padding: 1px 2px; line-height: 1; border-radius: 2px; position: relative; top: -1px; margin-left: 4px;}




/*stock 港美股标签*/
.stock_hk,.stock_us{position: relative;}
.stock_hk:after{ position: absolute; left: 0; top: 12px; content:''; width: 14px; height: 14px; background: url(data:image/png;base64,) 0 0 no-repeat; -webkit-background-size: 100% auto; background-size: 100% auto;}
.stock_us:after{ position: absolute; left: 0; top: 12px; content:''; width: 14px; height: 14px; background: url(data:image/png;base64,) 0 0 no-repeat; -webkit-background-size: 100% auto; background-size: 100% auto;}

/*大盘指标*/
.d3_chart{position: relative;}

.graphic{margin: 0 12px 14px; position: relative;}
.graphic_lines_hint{ margin: 5px 18px 0;}
.graphic_lines_hint .span{ padding: 4px; display: inline-block;}
.graphic_lines_hint .span:first-child{margin-left: -4px;}
.graphic_lines_hint .i{ font-size: 12px; display: inline-block; padding: 2px 7px; border-radius: 14px; height: 16px; line-height: 16px; cursor: pointer;}
.graphic_lines_hint .span:nth-child(1) .i{ color: #3C9FFC; border: 1px #3C9FFC solid;}
.graphic_lines_hint .span:nth-child(2) .i{ color: #BD0FE1; border: 1px #BD0FE1 solid;}
.graphic_lines_hint .span:nth-child(3) .i{ color: #FF5D33; border: 1px #FF5D33 solid;}
.graphic_lines_hint .span:nth-child(4) .i{ color: #3ec351; border: 1px #3ec351 solid;}
.graphic_lines_hint .span:nth-child(5) .i{ color: #ffae00; border: 1px #ffae00 solid;}
.graphic_lines_hint .span:nth-child(6) .i{ color: #8050e4; border: 1px #8050e4 solid;}
.graphic_lines_hint .span:nth-child(7) .i{ color: #ff2d55; border: 1px #ff2d55 solid;}
.graphic_lines_hint .span:nth-child(8) .i{ color:#00b8b6; border: 1px #00b8b6 solid;}

.d3_market_lines{padding-top: 70%;}
.d3_bars{padding-top: 65%;}
.d3_pie{padding-top: 62%;}
.d3_chip{ padding-top: 115%;}


/*行情里的入口图标*/
.entry_icons{ width: 100%; overflow: hidden; padding: 13px 0 11px; }
.entry_icons .ico_big{ float: left; width: 25%; text-align: center;  padding-top: 50px;}
.entry_icons .ico_big_live{ background: url(data:image/png;base64,) center 0 no-repeat; -webkit-background-size: 44px auto; background-size: 44px auto;}
.entry_icons .ico_big_chip_distribution{ background: url(data:image/png;base64,) center 0 no-repeat; -webkit-background-size: 44px auto; background-size: 44px auto;}
.entry_icons .ico_big_similark{ background: url(data:image/png;base64,) center 0 no-repeat; -webkit-background-size: 44px auto; background-size: 44px auto;}
.entry_icons .ico_big_history_check{ background: url(data:image/png;base64,) center 0 no-repeat; -webkit-background-size: 44px auto; background-size: 44px auto;}