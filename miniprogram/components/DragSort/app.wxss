.num_rise{ color: #e64b4b; font-family: 'avenir';}
.num_fall{ color: #4eb76c; font-family: 'avenir';}
.num_stop{ font-family: 'avenir';}*/

.num{}
.num_rise{ color: #e64b4b; }
.num_fall{ color: #4eb76c; }
.num_stop{ }



.bdb{border-bottom: 1px #efefef solid;}
.bdt{border-top: 1px #efefef solid;}
.bdr{border-right: 1px #efefef solid;}

.gap_line{ height: 1px; background-color: #efefef; margin: 14px 0; }

.no_data{ padding: 50px 0; text-align: center; font-size: 14px; color: rgba(89,89,89,.8); background-color: #fff;}
.display_center{ position:absolute; width:100%; height:100%; left:0; top:0; padding:0; display:-webkit-box; -webkit-box-align:center; -webkit-box-pack:center;}

.mobile_console{ position: fixed; width: 50%; height: 50%; right: 0; top: 0; z-index: 9999; background-color: rgba(0,0,0,.7); pointer-events: none; color: #fff;}

.load_more{ padding: 20px 0; text-align: center; color: rgba(89,89,89,.6); font-size: 14px; }

.bottom_gap{ height: 30px; }

.align_center{text-align: center;}

.risk_note{ padding: 15px 0; color: rgba(37,37,37,.6); text-align: center;}

/*通用字体缩小，只在小程序中使用*/
.font_reduce1{ font-size: 14px!important; }
.font_reduce2{ font-size: 12px!important; }

/*通用arrow上下箭头*/
.arrow_up:before,
.arrow_down:before{ content:''; display: inline-block; width: 0; height: 0; border: 3px transparent solid; margin-right: 3px; vertical-align: middle; position: relative;}
.arrow_up:before{ border-bottom: 6px #e64225 solid; top: -2px;}
.arrow_down:before{ border-top: 6px #0fa802 solid; top: 1px;}

.mobile_console{ position: fixed; right: 0; top: 0; z-index: 99999; width: 300px; height: 300px; background-color: rgba(0,0,0,.5); color: #fff; -ms-word-break: break-all; word-break: break-all;}

/*通用main_title*/
.main_title{ margin: 0 0 12px; padding: 10px 15px; font-size: 16px; overflow: hidden; position: relative; z-index: 20; color: rgba(37,37,37,1); clear: both;}
.main_title_strong{ font-weight: bold;}
.main_title strong,
.main_title .strong{font-weight: bold; float: left;}
.main_title span,
.main_title .span{float: right; font-size: 12px; color: rgba(37,37,37,.6); line-height: 1.4; margin: 3px 2px 0 0; font-weight: normal;}
.main_title a.link_detail{ float: right; color: rgba(37,37,37,.6); font-size: 12px; font-weight: normal; line-height: 39px; height: 39px; margin: -10px -16px -10px -15px; padding: 0 15px;}
.main_title_nomargin{margin-bottom: 0;}
.main_title_bg{background-color: #f0f0f0;}

.main_title_with_line{ margin: 12px 15px 14px; padding: 8px 0; font-size: 16px; overflow: hidden; position: relative; color: rgba(37,37,37,1); border-bottom: 1px #e4e4e4 solid;}
.main_title_with_line strong{ font-weight: bold; }

.sub_title{margin: 0 15px 10px; color: rgba(37,37,37,.8); font-size: 14px; -ms-word-break: break-all; word-break: break-all; line-height: 1.5;}
.cont_title{ margin-bottom: 10px; color: rgba(37,37,37,.8); font-size: 14px; line-height: 1.5; clear: both;}


/*通用简介类列表*/
.data_list{ margin: 0 15px 8px;}
.data_list li,
.data_list .li{ display:block; padding: 3px 0 13px; font-size: 14px; color: rgba(37,37,37,.8); display: table; width: 100%; line-height: 1.5;}
.data_list li span,
.data_list .li .span{ display: table-cell; vertical-align: top; white-space: nowrap; width: 110px;}
.data_list li strong,
.data_list li .strong,
.data_list .li strong{ display: table-cell; vertical-align: top; font-weight: normal; text-align: right; word-break: break-all; color: rgba(37,37,37,1);}
.data_list li strong.align_left,
.data_list .li .strong.align_left{text-align: left;}
.data_list li.no_break strong,
.data_list .li.no_break .strong{word-break:normal; }

.data_list_type2 li strong{text-align: left; color: #959595;}

.data_list_narrow li span{ width: 104px; }  /*两列的第一列偏窄，目前只用于定增详情*/


/*通用标签*/
.tag{display: inline-block; font-size: 10px; border: 1px rgba(152,152,152,.6) solid; border-radius: 3px; padding: 1px 2px 0; color: #989898; line-height: 1.2; vertical-align: top; margin-right: 4px;}
.tag1{ border-color: #e64225; color: #e64225;}
.tag2{ border-color: #f69300; color: #f69300;}
.tag3{ border-color: #989898; color: #989898;}
.tag4{ border-color: #0fa802; color: #0fa802;}
.tag5{ border-color: #066a5c; color: #066a5c;}


/*暂无的图标*/
.ico_no{ display: block; width: 175px; height: 130px; margin: 0 auto 10px;}


/*搜索条*/
.search_bar{ padding: 9px 15px; overflow: hidden; display: flex;}
.search_bar .input_holder{ flex:1; height: 34px; background-color: #fff; border-radius: 4px; position: relative;}
.search_bar .input_holder:before{content:''; position: absolute; width: 13px; height: 13px; display: block; left: 10px; top: 10px; background: url(data:image/png;base64,) 0 0 no-repeat; -webkit-background-size: 100% auto; background-size: 100% auto;}
.search_bar input{ width: 100%; box-sizing: border-box; height: 34px; line-height: 34px; padding-left: 28px; border-radius: 4px; font-size: 13px; background-color: transparent; color: #353535; position: relative;}
.search_bar .search_placeholder{ color: #b2b2b2;}
.search_bar .btn_cancel{ width: 0; height: 34px; line-height: 34px; color: #df3a32; border-radius: 4px; font-size: 14px; text-align: center; visibility: hidden;}
.search_bar .show_cancel{ width: 50px; margin-left: 10px; visibility: visible;}

.search_bar_white{background-color: #fff;}
.search_bar_white input{background-color: #f0f0f0;}

.search_navigator{ display: block; width: 100%; box-sizing: border-box; height: 34px; line-height: 34px; padding: 0 12px; background-color: #fff; color: #b2b2b2; border-radius: 4px; text-align: center;}
.search_navigator:before{content:''; display: inline-block; width: 13px; height: 13px; background:url(data:image/png;base64,); -webkit-background-size: 100% auto; background-size: 100% auto; vertical-align: middle; margin-right: 4px; position: relative; top: -1px;}



.news{ padding: 10px 15px 10px; border-bottom: 1px #eee solid; overflow: hidden;}
.news .news_title{ font-size: 17px; margin-bottom: 6px; line-height: 1.3;}
.news .news_txt{ font-size: 13px; color: #888; line-height: 1.4; margin-bottom: 8px; overflow : hidden; display: -webkit-box; -webkit-line-clamp: 3; -webkit-box-orient: vertical;}
.news .news_date{ float: right; font-size: 12px; color: #888; margin-left: 12px;}
.news .news_from{ float: right; font-size: 12px; color: #888;}

.news_tag{ float: left; font-size: 10px; padding-top: 2px;}
.news_tag .li{ float: left; width: 24px; height: 6px; margin-top: 4px; text-align: center;}
.news_tag .li1{ background-color: #2a8e29; border-radius: 6px 0 0 6px;}
.news_tag .li2{ background-color: #4cba2a; }
.news_tag .li3{ background-color: #c5d106; }
.news_tag .li4{ background-color: #ff9800; }
.news_tag .li5{ background-color: #ea4410; border-radius: 0 6px 6px 0;}

.news_tag1 .li5{ width: 48px; padding: 0 6px; height: 14px; line-height: 14px; margin: 0;}
.news_tag2 .li4{ width: 24px; padding: 0 6px; height: 14px; line-height: 14px; margin: 0;}
.news_tag3 .li3{ width: 24px; padding: 0 6px; height: 14px; line-height: 14px; margin: 0;}
.news_tag4 .li2{ width: 24px; padding: 0 6px; height: 14px; line-height: 14px; margin: 0;}
.news_tag5 .li1{ width: 48px; padding: 0 6px; height: 14px; line-height: 14px; margin: 0;}

.news_tag1 .li5:before{content:'强烈利好'; color: #fff;}
.news_tag2 .li4:before{content:'利好'; color: #fff;}
.news_tag3 .li3:before{content:'中性'; color: #fff;}
.news_tag4 .li2:before{content:'利空'; color: #fff;}
.news_tag5 .li1:before{content:'强烈利空'; color: #fff;}

.yan_tag.news_tag1 .li5:before{content:'买入'; color: #fff;}
.yan_tag.news_tag2 .li4:before{content:'增持'; color: #fff;}
.yan_tag.news_tag3 .li3:before{content:'中性'; color: #fff;}
.yan_tag.news_tag4 .li2:before{content:'减持'; color: #fff;}
.yan_tag.news_tag5 .li1:before{content:'卖出'; color: #fff;}
.yan_tag.news_tag1 .li1,
.yan_tag.news_tag1 .li5{width: 24px;}

.news_tag_arow{float: none; clear: both;}

.btn_more{ padding: 14px 0; text-align: center; font-size: 14px; color: rgba(89,89,89,0.6);}

.loading{text-align: center; height: 40px; padding-top: 5px; overflow: hidden; color: rgba(89,89,89,0.6);}
.loading .p{ display: inline; position: relative;}
.loading .i{ width: 15px; height: 15px; display: block; margin: 2px auto 0;
    background: url(data:image/png;base64,) 0 0 no-repeat; -webkit-background-size: 100%;}
.loading_big{padding: 50px 0;}
.loading_big .i{ width: 25px; height: 25px;}
.loading .span{ display: block; margin-top: 4px; font-size: 12px; line-height: 1;}
.rotating .i{ -webkit-animation: rotating 0.7s linear infinite; }
.loading_pure_txt .i{ display: none;}
.hide_rotating .span{ margin-top: 10px;}
.hide_rotating .i{display: none;}
@-webkit-keyframes rotating{
  0%{ -webkit-transform: rotate(0); -moz-transform: rotate(0); transform: rotate(0);}
  100% { -webkit-transform: rotate(360deg); -moz-transform: rotate(360deg); transform: rotate(360deg);}
}


/*================搜索start=================*/
.search_hint{ padding: 10px 0; text-align: center; color: rgba(59,59,59,.3); font-size: 12px;}
.search_hint .hint_title{ width: 200px; padding-bottom: 8px; border-bottom: 1px rgba(59,59,59,.05) solid; margin: 0 auto 20px;}
.search_hint .p{ margin-bottom: 20px;margin:0 auto 20px;}

.no_search_result{text-align: center; font-size: 14px; padding: 25px 0; color: rgba(89,89,89,0.6); background-color: #fff; line-height: 1.7;}
.search_list{ background-color: #fff; }
.search_list .li{ padding: 10px 15px; position: relative;}
.search_list .li .s_secname{ font-size: 16px; line-height: 1.1;}
.search_list .li .s_seccode{ font-size: 12px; color: rgba(89,89,89,.6);}
.search_list .li .s_add{position: absolute; width: 65px; height: 51px; overflow: hidden; right: 0; top: 0; text-align: center;}
.search_list .li.disabled .s_secname,
.search_list .li.disabled .s_seccode{ color: rgba(37,37,37,.6);}
.search_list .li.disabled:before{content:'暂不支持'; position: absolute; right: 10px; top: 18px; color: rgba(37,37,37,.4);}

.s_add_icon{ width: 50px; height: 50px; margin: 2px auto 0; background: url(data:image/png;base64,) 80% center no-repeat; -webkit-background-size: 25px auto;background-size: 25px auto;}


.search_list .li .s_added:before{background-color: transparent; width: auto; height: auto; color: #999; content:'已添加 '; white-space: nowrap; font-size: 14px; position: relative; top: 18px; right: 5px;}
.search_list .li .s_added .s_add_icon{ display: none;}

.search_list_title{ padding: 10px 15px; font-size: 14px; background-color: #f0f0f0; color: rgba(89,89,89,.8);}

.clear_list{ display: block; padding: 15px 0; text-align: center; font-size: 14px; color: rgba(89,89,89,.6);}
/*================搜索end=================*/


.top_canvas{ position: absolute; left: 0; top: 0; width: 100%; z-index: 10; }

