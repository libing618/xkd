
@font-face {
    font-family: 'avenir';
    src: url(data:application/x-font-ttf;base64,) format('truetype');              /* chrome、firefox、opera、Safari, Android, iOS 4.2+ */
    font-weight: normal;
    font-style: normal;
}

.hidden,.none{display: none!important;}
.unvisible { visibility: hidden!important; pointer-event: none!important; }
.clearfix:after{content:".";display:block;visibility:hidden;clear:both;height:0}

page { background-color: #f5f5f5; color: #373737; height: 100%; font-size: 12px;}
.cont_section{ background-color: #fff; margin-bottom: 8px; overflow: hidden;}
.cont_section_nomargin{margin-bottom: 0;}
.cont_txt{margin: 0 15px 14px; font-size: 14px; color: rgba(37,37,37,.8); line-height: 1.8; -ms-word-break: break-all; word-break: break-all; position: relative;}


/*默认为涨跌字体特殊，停牌字体普通*/
